{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "schema.gql"
      ],
      "dependsOn": [
        "generate"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ]
    },
    "bundle": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "dist/compiled/**"
      ]
    },
    "generate": {
      "outputs": [
        "src/graphql/generated/**"
      ],
      "dependsOn": [
        "^generate"
      ]
    }
  }
}
